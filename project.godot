[physics]

3d/run_on_separate_thread=true
common/physics_ticks_per_second=40
common/max_physics_steps_per_frame=6
3d/physics_engine="Jolt Physics"
jolt_physics_3d/simulation/velocity_steps=8
jolt_physics_3d/simulation/areas_detect_static_bodies=true
jolt_physics_3d/limits/temporary_memory_buffer_size=46
3d/solver/solver_iterations=8
common/physics_interpolation=true
jolt_3d/solver/velocity_iterations=5
jolt_3d/solver/position_iterations=1

[rendering]

textures/canvas_textures/default_texture_filter=0
textures/lossless_compression/force_png=true
lights_and_shadows/directional_shadow/size=8192
global_illumination/gi/use_half_resolution=true
anti_aliasing/quality/msaa_3d=2
anti_aliasing/screen_space_roughness_limiter/enabled=false
textures/decals/filter=0
textures/light_projectors/filter=0
global_illumination/sdfgi/frames_to_converge=0
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
anti_aliasing/quality/screen_space_aa=1
occlusion_culling/use_occlusion_culling=true
